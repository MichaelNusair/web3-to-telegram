{
  "name": "@michaelnusair/web3-to-telegram",
  "version": "1.0.1",
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "src/frontend"
  ],
  "scripts": {
    "build": "rm -rf build && tsc",
    "cdk:list": "cdk list",
    "deploy:prod": "cdk deploy web3-to-telegram --require-approval never",
    "watch": "tsc -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cdk:dry-run": "rm -rf build/bundles && npm --workspace frontend run build && cdk synth",
    "test": "jest"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@nx/eslint-plugin": "^20.0.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^24.1.0",
    "@typescript-eslint/parser": "^8.0.0",
    "aws-cdk": "2.147.3",
    "axios": "^1.7.2",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.11.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.0.0",
    "@aws-cdk/aws-apigatewayv2-alpha": "2.114.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.114.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "2.114.1-alpha.0",
    "@swc/jest": "^0.2.36",
    "jest": "^29.7.0",
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.147.3",
    "constructs": "^10.0.0",
    "eslint": "^9.32.0",
    "ethers": "^6.15.0"
  }
}
